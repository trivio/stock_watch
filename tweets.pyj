bac_tweets = rule('http://search.twitter.com/search.json?q=$BAC', 'bac_tweets')

@bac_tweets.map
def map(doc, params):
  import json
  for tweet in  json.loads(doc)['results']:
    yield tweet['created_at'], tweet['text']

@bac_tweets.reduce
def reduce(iter, params):
  for k,v in iter:
    yield k,v

